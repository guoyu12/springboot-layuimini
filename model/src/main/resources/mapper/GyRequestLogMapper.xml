<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anshark.mapper.GyRequestLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.anshark.model.GyRequestLog">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="code" property="code" />
        <result column="request_uri" property="requestUri" />
        <result column="ip_addr" property="ipAddr" />
        <result column="request_param" property="requestParam" />
        <result column="response_msg" property="responseMsg" />
        <result column="response_data" property="responseData" />
        <result column="trace_stack" property="traceStack" />
        <result column="create_at" property="createAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, code, request_uri, ip_addr, request_param, response_msg, response_data, trace_stack, create_at
    </sql>

</mapper>
